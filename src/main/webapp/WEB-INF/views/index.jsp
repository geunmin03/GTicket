    function onYouTubeIframeAPIReady() {
        playerFocusOn = new YT.Player('playerFocusOn', {
            videoId: '',
            events: {
                'onReady': onPlayerReady,
                'onStateChange': onPlayerStateChange
            }
        });

        playerBigBanner = new YT.Player('playerBigBanner', {
            videoId: '',
            events: {
                'onReady': onPlayerReady,
                'onStateChange': onPlayerStateChange
            }
        });
    }

    function onPlayerReady(event) {
        event.target.stopVideo();
    }

    function onPlayerStateChange(event) {
        playerState = event.data == YT.PlayerState.ENDED ? '종료됨 (0)' :
                              event.data == YT.PlayerState.PLAYING ? '재생 중 (1)' :
                              event.data == YT.PlayerState.PAUSED ? '일시중지 됨 (2)' :
                              event.data == YT.PlayerState.BUFFERING ? '버퍼링 중 (3)' :
                              event.data == YT.PlayerState.CUED ? '동영상 신호 (5)' :
                              event.data == -1 ? '시작되지 않음 (-1)' : '예외';

        if (event.data == YT.PlayerState.ENDED) {
            if (event.target.a.id == "playerFocusOn")
                $(".focus-con-img").fadeIn();

            if (event.target.a.id == "playerBigBanner")
                $(".movie-close").click();
